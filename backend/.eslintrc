{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }], // Permitir el uso de _id
    "no-console": "off", // Permitir console.log
    "linebreak-style": ["error", "unix"], // Usar saltos de l√≠nea estilo Unix (\n)
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
